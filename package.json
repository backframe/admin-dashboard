{
  "name": "backframe-admin-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "The backframe admin dashboard user interface",
  "homepage": "https://backframe.dev",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./src/**/*.{ts,tsx} ./**/*.{js,ts}",
    "format:check": "prettier --check ./src/**/*.{ts,tsx} ./**/*.{js,ts}",
    "check:all": "pnpm lint && pnpm format:check"
  },
  "dependencies": {
    "@formkit/auto-animate": "1.0.0-beta.3",
    "@headlessui/react": "^1.7.3",
    "@tabler/icons": "^1.105.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "chart.js": "^3.9.1",
    "clsx": "^1.2.1",
    "d3": "^7.6.1",
    "framer-motion": "^7.5.4",
    "leaflet": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-leaflet": "^4.1.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "crossenv": "0.0.2-security",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
